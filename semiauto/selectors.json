{
  "base_url": "https://jangboo.grider.ai",
  "login": {
    "url_path": "/login",
    "id_input": "input#user_id",
    "pw_input": "input#user_pw",
    "login_button": "button.login_btn"
  },
  "daily_data": {
    "url_path": "/dashboard",
    "container": "div.rider_list",
    "item": ".rider_item",
    "name": ".rider_name",
    "user_id": ".user_id",
    "status": ".working_status .rider_info_text",
    "complete_count": ".complete_count",
    "reject_count": ".reject_count",
    "accept_cancel_count": ".accept_cancel_count",
    "accept_cancel_rider_fault_count": ".accept_cancel_rider_fault_count"
  },
  "weekly_mission_data": {
    "url_path": "/orders/sla/list",
    "summary": {
      "container": "div.summary_area",
      "total_score": "span.summary_score_text[data-text='total']",
      "quantity_score": "span.summary_score_text[data-text='quantity']",
      "acceptance_score": "span.summary_score_text[data-text='acceptance']"
    },
    "weekly_rider_list": {
      "container": "div.rider_list",
      "item": ".rider_item"
    },
    "mission_table": {
      "container": "table.sla_table",
      "row": "tbody tr"
    }
  }
} 